function  addFav() {   //加入收藏夾   
             if  (document.all) {   
                window.external.addFavorite('http://www.takungpao.com/', '大公網(忘已之為大，無私之謂公)');   
            }   
             else   if  (window.sidebar) {   
            window.sidebar.addPanel('大公網(忘已之為大，無私之謂公)', 'http://www.takungpao.com/',  "" );   
            }   
        }   
function SetHome(obj){   
    try{   
        obj.style.behavior='url(#default#homepage)';   
        obj.setHomePage('http://www.takungpao.com/');   
    }catch(e){   
        if(window.netscape){   
            try{   
                netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");   
            }catch(e){   
                alert("抱歉，此操作被瀏覽器拒絕！\n\n請在瀏覽器地址欄輸入“about:config”並回車然後將[signed.applets.codebase_principal_support]設置為'true'");   
            };   
        }else{   
            alert("抱歉，您所使用的瀏覽器無法完成此操作。\n\n您需要手動將'http://www.takungpao.com/'設置為首頁。");   
        };   
    };   
};   
document.domain = "takungpao.com";
function setWinHeight(crior){
		var win=document.getElementById("win");
		if (document.getElementById){ 
			if (win && !window.opera){ 
				if (win.contentDocument && win.contentDocument.body.offsetHeight){
					if(crior=="nocri"){
						win.height = 0;
					}else{
						win.height = win.contentDocument.body.offsetHeight+40;
					}
					
				}else if(win.Document && win.Document.body.scrollHeight){
					if(crior=="nocri"){
						win.height = 0;
					}else{
						win.height = win.Document.body.scrollHeight+40;
					}					
				}					
			} 
		} 
	}
</script>
