      texttop = texttop +"<ul>\r\n";
      var text = "";
      for (var i=0 ;i<6;i++){
        var code = board_shares.shares[i].code
        var name = board_shares.shares[i].name
        var np = board_shares.shares[i].np
        var pl = board_shares.shares[i].pl
        var hlp = board_shares.shares[i].hlp					
        text = text + "<li><a href=\"http://stock.jrj.com.cn/cominfo/default_"+code+".htm\">"+name+"</a>";
        if(hlp>0){
          text = text +"<span class=\"rd\">"+np+"</span><span class=\"rd\">"+pl+"%</span>";	
        } else {
          text = text +"<span class=\"gr\">"+np+"</span><span class=\"gr\">"+pl+"%</span>";
        }
        text = text + "</li>"
      }
      var textbot = "</ul>\r\n";
      textbot = textbot+"<p class=\"tr\">行情截止时间 "+board_shares.summary.time+" </p>\r\n";
      textbot = textbot+"</div>\r\n";
      textbot = textbot+"</div>\r\n";
      document.getElementById("stocklisthtml").innerHTML=texttop+text+textbot;
      //document.write(texttop+text+textbot);
    }else{
      document.getElementById("stocklisthtml").innerHTML="";
    }
  }catch(e){
    document.getElementById("stocklisthtml").innerHTML="";
    //alert(e);
  }
}else{
  if(document.getElementById("stocklisthtml"))
  document.getElementById("stocklisthtml").innerHTML="";
}
